{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
  {
    "src": "/admin",
    "dest": "/public/admin.html"
  },
  {
    "src": "/api/(.*)",
    "dest": "/server.js"
  },
  {
    "src": "/(.*)",
    "dest": "/public/$1"
  }
]

}